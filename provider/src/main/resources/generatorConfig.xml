<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="MySQLTables" targetRuntime="MyBatis3">

        <property name="autoDelimitKeywords" value="true"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306?useUnicode=true&amp;zeroDateTimeBehavior=CONVERT_TO_NULL&amp;autoReconnect=true&amp;useSSL=false&amp;serverTimezone=GMT%2B8"
                        userId="root"
                        password="www1027325253">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="provider.domain"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="provider.repository.mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="community" tableName="discuss_postt" domainObjectName="DiscussPostDO">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>

        <table schema="community" tableName="commentt" domainObjectName="CommentDO">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>

        <table schema="community" tableName="login_tickett" domainObjectName="TicketDO">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>

        <table schema="community" tableName="messagee" domainObjectName="MessageDO">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>

        <table schema="community" tableName="user11" domainObjectName="UserDO">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>
    </context>

</generatorConfiguration>
